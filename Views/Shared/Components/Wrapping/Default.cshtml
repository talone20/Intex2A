@{
}

@model IEnumerable<string>
<div class="d-grid">
    @foreach (string w in Model)
    {
        <a asp-controller="Home" asp-action="Summary" asp-route-wrapping="@w" asp-route-pageNum="1"
           class="btn @(ViewBag.Wrapping == w ? "btn-primary" : "btn-outline-secondary")" >@w</a>
    }
</div>